{
  "openapi": "3.0.1",
  "info": {
    "title": "Auth Contexts API",
    "description": "Auth Contexts API for Open Banking Connector Web App",
    "version": "web-app/4.0.0"
  },
  "paths": {
    "/auth/query-redirect": {
      "get": {
        "tags": [
          "OAuth2 Redirects"
        ],
        "summary": "OAuth2 query redirect endpoint",
        "parameters": [
          {
            "name": "id_token",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "code",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "state",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/auth/redirect-delegate": {
      "post": {
        "tags": [
          "OAuth2 Redirects"
        ],
        "summary": "Delegate endpoint for forwarding data captured elsewhere from OAuth2 redirect",
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "id_token": {
                    "type": "string"
                  },
                  "code": {
                    "type": "string"
                  },
                  "state": {
                    "type": "string"
                  },
                  "response_mode": {
                    "type": "string"
                  },
                  "modified_by": {
                    "type": "string"
                  },
                  "redirect_uri": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "id_token": {
                  "style": "form"
                },
                "code": {
                  "style": "form"
                },
                "state": {
                  "style": "form"
                },
                "response_mode": {
                  "style": "form"
                },
                "modified_by": {
                  "style": "form"
                },
                "redirect_uri": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/AuthContextUpdateAuthResultResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthContextUpdateAuthResultResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthContextUpdateAuthResultResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AuthContextUpdateAuthResultResponse": {
        "type": "object",
        "properties": {
          "ConsentType": {
            "$ref": "#/components/schemas/ConsentType"
          },
          "ConsentId": {
            "type": "string",
            "format": "uuid"
          },
          "Warnings": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Optional list of warning messages from Open Banking Connector.",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ConsentType": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      }
    }
  }
}